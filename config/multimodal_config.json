{
  "multimodal_settings": {
    "enable_image_extraction": true,
    "enable_image_classification": true,
    "max_images_per_chunk": 5,
    "image_confidence_threshold": 0.5,
    "enable_vision_model_routing": false,
    "vision_model_api_key": null
  },
  "image_classification": {
    "high_priority_types": [
      "chart_or_graph",
      "diagram",
      "technical_drawing",
      "document_scan",
      "screenshot",
      "scientific_figure",
      "flowchart",
      "blueprint"
    ],
    "confidence_thresholds": {
      "high": 0.8,
      "medium": 0.5,
      "low": 0.3
    }
  },
  "processing_settings": {
    "chunk_image_association_distance": 1,
    "enable_enhanced_embeddings": true,
    "save_image_metadata": true,
    "extract_image_descriptions": true
  },
  "output_directories": {
    "extracted_images": "extracted_content/images",
    "multimodal_analysis": "multimodal_analysis",
    "image_cache": "cache/images"
  },
  "supported_formats": {
    "input": ["pdf", "txt"],
    "image_output": ["png", "jpg", "jpeg"],
    "text_output": ["txt", "json"]
  }
}
